							{
								"key": "x-api-key",
								"value": "{{api_key}}",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n \"commands\": [\n   {\n     \"insert\": {\n       \"object\": {\n         \"accessnyc.request.Household\": {\n           \"members\": \"1\",\n           \"cashOnHand\": \"50000\",\n           \"livingRentalType\": \"RentControlled\",\n           \"livingRenting\": \"true\",\n           \"livingOwner\": \"false\",\n           \"livingStayingWithFriend\": \"false\",\n           \"livingHotel\": \"false\",\n           \"livingShelter\": \"false\",\n           \"livingPreferNotToSay\": \"false\"\n         }\n       }\n     }\n   },\n   {\n     \"insert\": {\n       \"object\": {\n         \"accessnyc.request.Person\": {\n           \"age\": \"35\",\n           \"incomes\": [\n             {\n               \"amount\": \"200\",\n               \"type\": \"Veteran\",\n               \"frequency\": \"monthly\"\n             }\n           ],\n           \"expenses\": [\n             {\n               \"amount\": \"50\",\n               \"type\": \"Medical\",\n               \"frequency\": \"weekly\"\n             }\n           ],\n           \"student\": \"false\",\n           \"studentFulltime\": \"false\",\n           \"pregnant\": \"false\",\n           \"unemployed\": \"false\",\n           \"unemployedWorkedLast18Months\": \"false\",\n           \"blind\": \"false\",\n           \"disabled\": \"false\",\n           \"veteran\": \"true\",\n           \"benefitsMedicaid\": \"false\",\n           \"benefitsMedicaidDisability\": \"false\",\n           \"headOfHousehold\": \"true\",\n           \"livingOwnerOnDeed\": \"false\",\n           \"livingRentalOnLease\": \"true\"\n         }\n       }\n     }\n   }\n ]\n}"
						},
						"url": {
							"raw": "https://{{domain}}/eligibilityPrograms",
							"protocol": "https",
							"host": [
								"{{domain}}"
							],
							"path": [
								"eligibilityPrograms"
							]
						}
					},
					"response": []
				}
			],
			"description": "This group contains tests for the Eligibility Programs (single request) endpoint. Authorization is managed by the Pre-request Script setting the appropriate environment variables for {{username}} and {{token}}. "
		},
