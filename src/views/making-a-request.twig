{% extends "@src/twig/layouts/default.twig" %}

{% set title = 'Making a Request' %}
{% set page_title = title %}

{% block container %}
<section>
	<div class="layout--content pt-4">
		<div class="screen-desktop:pr-4 screen-desktop:pr-6">
			{% if title %}<h1 class="mt-0 p-0">{{ title }}</h1>{% endif %}
			<div class="o-content">
        <div id="markdown-making-a-request-1"></div>
			</div>
		</div>
	</div>

	<div class="relative overflow-x-auto scrolling-touch">
    <div id="markdown-household-composition-household-table" class="table"></div>
  </div>

  <div class="relative overflow-x-auto scrolling-touch">
    <div id="markdown-household-composition-person-table" class="table"></div>
	</div>

	<div class="layout--content mt-4 pb-4">
		<div class="screen-desktop:pr-4 screen-desktop:pr-6">
			<div class="o-content">
        <div id="markdown-making-a-request-2"></div>
			</div>
		</div>
	</div>
</section>
{% endblock %}

