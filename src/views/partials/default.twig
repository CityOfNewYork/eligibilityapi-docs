  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
  <meta name="viewport" content="width=device-width,initial-scale=1">
  
  <title>Eligibility API{% if page_title %}{{ page_title }}{% endif %}</title>

  <link rel="stylesheet" type="text/css" href="css/style.css">
  
  {% block head %}{% endblock %}
</head>

<body>
	<header class="p-2">
	  <h1 class="font-sans text-color-blue-dark">
	    NYC Benefits Eligibility API <a class="version" href="https://github.com/CityOfNewYork/eligibilityapi">1.0.0</a>
	  </h1>
	</header>

	<section class="usa-grid screen-desktop:layout--sidebar-small-gutter">
	  <aside>
	    {% include "nav.twig" %}
	  </aside>

    <div class="font-sans">
    {% block content %}{% endblock %}
    </div>
	
	</section>

	<script type="text/javascript" src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
	
	<script src="js/source.js"></script>

</body>
</html>